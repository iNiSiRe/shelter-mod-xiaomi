{
    "name": "inisire/xiaomi-gateway",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "binsoul/net-mqtt-client-react": "^0.7.3",
        "inisire/netbus": "dev-master",
        "react/datagram": "*",
        "symfony/event-dispatcher": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/inisire/netbus"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "inisire/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Shelter\\Module\\Xiaomi\\": "src/"
        }
    }
}
